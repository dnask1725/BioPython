prot = ""
length = len(prot)
count = 0
for i in range(0,length):
    if prot[i]!="G" or prot[i]!="A" or prot[i]!="V" or prot[i]!="L" or prot[i]!="I" or prot[i]!="P" or prot[i]!="F" or prot[i]!="M" or prot[i]!="W":
       count = count+1
    else:
       pass
print("The hydrophilic amino acids are: ",count)
